local teleportTab = Window:MakeTab({
	Name = "teleport",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

teleportTab:AddButton({
	Name = "teleport to city",
	Callback = function()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-577.49353, 4.64937019, 363.274475, 0.993908703, 1.34933664e-09, -0.110206693, 9.52308632e-09, 1, 9.81284813e-08, 0.110206693, -9.85802586e-08, 0.993908703)
wait()
      		print("button pressed")
  	end    
})
teleportTab:AddButton({
	Name = "teleport to snowcity",
	Callback = function()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-885.145142, 4.69937038, 2161.77905, -0.259293556, -5.80951784e-08, 0.965798557, 2.83314794e-08, 1, 6.77587977e-08, -0.965798557, 4.49319231e-08, -0.259293556)
wait()
      		print("button pressed")
  	end    
})

