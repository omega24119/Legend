})


--values
getgenv().raid=false
getgenv().t=false
getgenv().gem = false
getgenv().speed = false
getgenv().exp = false
getgenv().a = false
getgenv().b = false
getgenv().c = false
getgenv().d = false
getgenv().e = false
getgenv().h = false
getgenv().i = false
getgenv().l = false
getgenv().k = false
getgenv().re = false
getgenv().bl = false
