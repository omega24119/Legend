--toggles
mainTab:AddToggle({
  Name = "auto race join",
  Default = false,
  Callback = function(bool)
    getgenv().raid = bool
    if bool then 
      doraid()
    end
  end})

mainTab:AddToggle({
  Name = "auto rebrith",
  Default = false,
  Callback = function(bool)
    getgenv().t = bool
    if bool then 
      dot()
    end
  end})
  
 
  
